{
  "rewrites": [
    {
      "source": "/api/:splat",
      "destination": "http://43.136.45.112/api/:splat"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Referrer-Policy",
          "value": "no-referrer-when-downgrade"
        }
      ]
    }
  ]
}

